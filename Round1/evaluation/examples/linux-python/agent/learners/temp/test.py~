import mdp
my_learner = mdp.mdpAgent()

output = my_learner.charOut("L")
print("output = ", output)
my_learner.rewardIn(-1)
output = my_learner.charOut("a")
print("output = ", output)
my_learner.rewardIn(-1)
output = my_learner.charOut(";")
print("output = ", output)
my_learner.rewardIn(-1)
output = my_learner.charOut("b")
print("output = ", output)
my_learner.rewardIn(0)
output = my_learner.charOut(";")
print("output = ", output)
my_learner.rewardIn(-1)
